{
  "name": "@project/lambda",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "clean": "rm -rf terraform/.terraform terraform/terraform.tfstate && docker compose restart",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "vitest --config vitest.config.e2e.mts run",
    "build": "tsup --config tsup.config.ts",
    "build:deploy": "tsup --config tsup.config.ts -- --deploy",
    "watch": "tsup --config tsup.config.ts --watch",
    "start": "yarn watch",
    "check:localstack": "localstack config validate"
  },
  "dependencies": {
    "@project/controllers": "workspace:*",
    "@tsed/ajv": "7.76.2",
    "@tsed/core": "7.76.2",
    "@tsed/di": "7.76.2",
    "@tsed/engines": "7.76.2",
    "@tsed/exceptions": "7.76.2",
    "@tsed/json-mapper": "7.76.2",
    "@tsed/logger": "6.7.5",
    "@tsed/logger-file": "6.7.5",
    "@tsed/openspec": "7.76.2",
    "@tsed/platform-exceptions": "7.76.2",
    "@tsed/platform-params": "7.76.2",
    "@tsed/platform-response-filter": "7.76.2",
    "@tsed/platform-serverless": "7.76.2",
    "ajv": "^8.12.0",
    "find-my-way": "8.1.0"
  },
  "devDependencies": {
    "@project/esbuild-lambda-plugin": "workspace:*",
    "@tsed/platform-serverless-testing": "^7.76.2",
    "@types/aws-lambda": "^8.10.141",
    "@types/supertest": "6.0.2",
    "concurrently": "8.2.2",
    "cross-env": "^7.0.3",
    "cross-zip": "4.0.1",
    "esbuild": "0.20.2",
    "esbuild-plugin-tsc": "^0.4.0",
    "globby": "14.0.1",
    "supertest": "6.3.4",
    "tslib": "2.6.3",
    "tsup": "8.1.0",
    "typescript": "5.4.5",
    "vitest": "^1.6.0"
  }
}
